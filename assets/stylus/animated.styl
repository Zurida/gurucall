.animated {
  animation-duration 1s
  animation-fill-mode both
  @media (print), (prefers-reduced-motion) {
    animation unset !important
    transition none !important
  }
}

@keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

.fadeIn {
  animation-name fadeIn
}